class Solution {
    static boolean isPerfect(int n) {
        // code here
        int c=1;
        for(int i=2;i*i<=n;i++){
            if(n%i==0)
            {
                c+=i;
                if(i!=n/i)
                c+=n/i;
            }
            
        }
        return c==n;
    }
};
